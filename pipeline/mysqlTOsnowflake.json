{
	"name": "mysqlTOsnowflake",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "MySqlSource"
					},
					"sink": {
						"type": "SnowflakeSink",
						"importSettings": {
							"type": "SnowflakeImportCopyCommand"
						}
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "linkedService1",
							"type": "LinkedServiceReference"
						},
						"path": ""
					}
				},
				"inputs": [
					{
						"referenceName": "MySqlTable1",
						"type": "DatasetReference",
						"parameters": {
							"tname": {
								"value": "@pipeline().parameters.tname",
								"type": "Expression"
							},
							"dbname": {
								"value": "@pipeline().parameters.dbname",
								"type": "Expression"
							},
							"uname": {
								"value": "@pipeline().parameters.uname",
								"type": "Expression"
							},
							"pwd": {
								"value": "@pipeline().parameters.pwd",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "SnowflakeTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"tname": {
				"type": "String"
			},
			"dbname": {
				"type": "String"
			},
			"uname": {
				"type": "String"
			},
			"pwd": {
				"type": "String"
			}
		},
		"annotations": []
	}
}